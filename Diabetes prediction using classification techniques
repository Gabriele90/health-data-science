      "source": [
        "Q1 = data_2.quantile(0.25)\n",
        "Q2 = data_2.quantile(0.75)\n",
        "IQR = Q2 - Q1\n",
        "IQR"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "background_save": true
        },
        "id": "dmk7Qs2Xothn"
      },
      "source": [
        "data2_out = data_2[~((data_2 < (Q1 - 1.5 * IQR)) |(data_2 > (Q2 + 1.5 * IQR))).any(axis=1)]"
